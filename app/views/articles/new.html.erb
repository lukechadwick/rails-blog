<h1>Create an Article</h1>


<%= form_for @article do |f|%>
    <div>
        <%= f.label :title %><br>
        <%= f.text_field :title %>
    </div>
    <br>
    <div>
        <%= f.label :description %><br>
        <%= f.text_area :description %>
        </div>
    <br>
    <div>
        <%= f.submit %><br>
    </div>

<%end%>